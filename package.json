{
  "name": "msgsystec",
  "version": "1.0.0",
  "description": "Sistema de autenticação Google e leitura de planilhas",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd apps/backend && npm run dev",
    "dev:frontend": "cd apps/web && npm run dev",
    "build": "npm run build:shared && npm run build:backend && npm run build:frontend",
    "build:shared": "cd packages/shared && npm run build",
    "build:backend": "cd apps/backend && npm run build",
    "build:frontend": "cd apps/web && npm run build",
    "start": "cd apps/backend && npm run start",
    "install:all": "npm install && cd apps/backend && npm install && cd ../web && npm install && cd ../../packages/shared && npm install",
    "clean": "npm run clean:shared && npm run clean:backend && npm run clean:frontend",
    "clean:shared": "cd packages/shared && npm run clean",
    "clean:backend": "cd apps/backend && npm run clean",
    "clean:frontend": "cd apps/web && rm -rf .next",
    "db:generate": "cd apps/backend && npm run db:generate",
    "db:push": "cd apps/backend && npm run db:push",
    "db:migrate": "cd apps/backend && npm run db:migrate",
    "db:studio": "cd apps/backend && npm run db:studio"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
